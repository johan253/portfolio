{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "api/main.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/docs",
      "dest": "api/main.py"
    },
    {
      "src": "/docs/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/openapi.json",
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
